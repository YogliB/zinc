{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"ignoreExportsUsedInFile": {
		"interface": true,
		"type": true,
		"function": true
	},
	"ignore": ["tests/setup/vitest-compat.ts"],
	"tags": ["-lintignore"],
	"workspaces": {
		"packages/core": {
			"entry": ["src/server.ts"],
			"project": ["src/**/*.ts", "tests/**/*.ts", "scripts/**/*.ts"]
		},
		"packages/dashboard": {
			"entry": [
				"src/routes/**/*.svelte",
				"src/routes/**/*.ts",
				".storybook/main.ts",
				"vite.config.ts",
				"svelte.config.js"
			],
			"project": ["src/**/*.{ts,svelte}", ".storybook/**/*.ts"]
		}
	}
}
