{
	"context_servers": {
		"devflow": {
			"source": "custom",
			"command": "devflow",
			"args": ["serve", "--stdio"],
			"env": {
				"DEVFLOW_ROOT": "/path/to/your/project"
			}
		}
	},
	"agent": {
		"profiles": {
			"default": {
				"enable_all_context_servers": true,
				"context_servers": {
					"devflow": {
						"tools": {
							"memory-get": true,
							"memory-save": true,
							"memory-list": true,
							"memory-delete": true,
							"memory-init": true,
							"memory-context": true,
							"memory-update": true
						}
					}
				}
			}
		}
	}
}
